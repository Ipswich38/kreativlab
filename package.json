{
  "name": "kreativlab",
  "version": "1.0.0",
  "private": true,
  "description": "Dental Administrative Support CRM - SaaS platform for dental practice management",
  "author": "KreativLab Team",
  "homepage": "https://github.com/Ipswich38/kreativlab",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ipswich38/kreativlab.git"
  },
  "keywords": [
    "crm",
    "dental",
    "saas",
    "practice-management",
    "call-center"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Warning: no tests specified\" && exit 0",
    "test:unit": "echo \"Warning: no unit tests specified\" && exit 0",
    "test:integration": "echo \"Warning: no integration tests specified\" && exit 0",
    "test:e2e": "echo \"Warning: no e2e tests specified\" && exit 0",
    "test:coverage": "echo \"Warning: no coverage tests specified\" && exit 0",
    "db:types": "supabase gen types typescript --local > src/lib/supabase/types.ts",
    "db:migrate": "supabase migration up",
    "db:reset": "supabase db reset",
    "db:seed": "psql $DATABASE_URL -f supabase/migrations/002_seed_data.sql",
    "docker:build": "docker build -t kreativlab .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "vercel:deploy": "vercel --prod"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.0",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query": "^5.90.5",
    "@trpc/client": "^11.6.0",
    "@trpc/next": "^11.6.0",
    "@trpc/react-query": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "autoprefixer": "^10.4.21",
    "lucide-react": "^0.468.0",
    "next": "15.5.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwindcss": "^4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.5",
    "superjson": "^2.2.2",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
