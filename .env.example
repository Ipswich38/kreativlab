# Database
DATABASE_URL="postgresql://username:password@localhost:5432/kreativlab?schema=public"

# Authentication
JWT_SECRET="your-super-secure-jwt-secret-key-here"
NEXTAUTH_SECRET="your-nextauth-secret-here"
NEXTAUTH_URL="http://localhost:3000"

# Redis (for caching and session storage)
REDIS_URL="redis://localhost:6379"

# MCP Server Configuration
MCP_SERVER_PORT=8080
MCP_CLIENT_TIMEOUT=30000

# Agent Execution Limits
MAX_AGENTS_PER_USER=10
MAX_MEMORY_PER_AGENT=128
MAX_EXECUTION_TIME=30000
MAX_CONCURRENT_EXECUTIONS=5

# Security
SECURITY_AUDIT_RETENTION_DAYS=90
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Email (for notifications)
SMTP_HOST="smtp.gmail.com"
SMTP_PORT=587
SMTP_USER="your-email@gmail.com"
SMTP_PASS="your-app-password"

# File Storage (for agent code and assets)
AWS_ACCESS_KEY_ID="your-aws-access-key"
AWS_SECRET_ACCESS_KEY="your-aws-secret-key"
AWS_S3_BUCKET="kreativlab-agents"
AWS_REGION="us-east-1"

# Monitoring
SENTRY_DSN="your-sentry-dsn-here"
LOG_LEVEL="info"

# Development
NODE_ENV="development"
PORT=3000

# Docker Registry (for containerized agents)
DOCKER_REGISTRY_URL="your-registry-url"
DOCKER_REGISTRY_USERNAME="your-username"
DOCKER_REGISTRY_PASSWORD="your-password"

# Webhook URLs (for external integrations)
WEBHOOK_SECRET="your-webhook-secret"
SLACK_WEBHOOK_URL="your-slack-webhook-url"

# Feature Flags
ENABLE_AGENT_MARKETPLACE=true
ENABLE_DOCKER_EXECUTION=false
ENABLE_TELEMETRY=true
ENABLE_RATE_LIMITING=true